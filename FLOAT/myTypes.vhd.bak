library ieee;
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;


package myTypes is

	type float4x32 is record
		a : std_logic_vector(31 downto 0);
		b : std_logic_vector(31 downto 0);
		c : std_logic_vector(31 downto 0);
		d : std_logic_vector(31 downto 0);
	end record;

	type std_logic_vector_2x32 is record
		a : std_logic_vector(31 downto 0);
		b : std_logic_vector(31 downto 0);
	end record;

	type std_logic_vector_3x32 is record
		a : std_logic_vector(31 downto 0);
		b : std_logic_vector(31 downto 0);
		c : std_logic_vector(31 downto 0);
	end record;

	type std_logic_vector_4x32 is record
		a : std_logic_vector(31 downto 0);
		b : std_logic_vector(31 downto 0);
		c : std_logic_vector(31 downto 0);
	end record;

end myTypes;